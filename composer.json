{
	"name": "tectonic/laravel-localisation",
	"description": "A bridge package for Localisation and Laravel packages, bringing translation code for models and collections to Laravel 4/5.",
	"authors": [
		{
			"name": "Kirk Bushell",
			"email": "kirk@tectonic.com.au"
		}
	],
	"repositories": [
		{
			"type": "git",
			"url": "git@github.com:kontoulis/localisation.git"
		}
	],
	"require": {
		"php": "^7.2",
		"illuminate/support": "^6.1",
		"illuminate/database": "^6.1",
		"tectonic/localisation": "dev-feature/php7.2-upgrade"
	},
	"require-dev": {
		"mockery/mockery": "^1.4",
		"orchestra/testbench": "4.*",
		"phpunit/phpunit": "^8.0"
	},
	"autoload": {
		"psr-4": {
			"Tectonic\\LaravelLocalisation\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\": "tests/"
		}
	},
	"config": {
		"preferred-install": "dist",
		"sort-packages": true
	}
}
