{
	"name": "tectonic/laravel-localisation",
	"description": "A bridge package for Localisation and Laravel packages, bringing translation code for models and collections to Laravel 4/5.",
	"authors": [
		{
			"name": "Kirk Bushell",
			"email": "kirk@tectonic.com.au"
		}
	],
	"require": {
		"php": ">=5.6.0",
		"illuminate/support": "5.*",
		"illuminate/database": "5.*",
		"tectonic/localisation": "1.0.*"
	},
	"require-dev": {
		"mockery/mockery": "dev-master@dev",
		"orchestra/testbench": "3.*",
		"phpunit/phpunit": "4.2.*"
	},
	"autoload": {
		"psr-4": {
			"Tectonic\\LaravelLocalisation\\": "src/Tectonic/LaravelLocalisation/",
			"Tests\\": "tests/"
		}
	},
	"minimum-stability": "stable"
}
